{
  "name": "rx-firebase",
  "version": "0.3.0",
  "description": "Extends Firebase websocket client with RxJS methods.",
  "main": "src/index.js",
  "directories": {
    "lib": "src"
  },
  "repository": "github:dinoboff/rx-firebase",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha tests/index.js --report lcov -- -R spec",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha tests/index.js --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "lint": "eslint src/ tests/",
    "lint-fix": "eslint src/ tests/ --fix",
    "test": "mocha -b tests/index.js"
  },
  "keywords": [
    "RxJS",
    "Firebase"
  ],
  "author": "Damien Lebrun",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "body": "^5.1.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.6",
    "eslint": "^2.11.1",
    "eslint-config-xo": "^0.14.1",
    "eslint-plugin-babel": "^3.2.0",
    "istanbul": "^0.4.3",
    "jspm": "^0.17.0-beta.17",
    "mocha": "^2.5.3",
    "remap-istanbul": "^0.6.4",
    "rxjs": "^5.0.0-beta.8",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "source-map-support": "^0.4.0"
  }
